#!/usr/bin/gnuplot

set terminal postscript eps size 3.2,2 enhanced
set output 'success_tes.eps'
#set terminal png size 500,500
#set output 'success.png'
set datafile separator ","
set xlabel "shaking factor"
set ylabel "OCR success rate (%)"

plot for [IDX=0:5] 'output_tes.csv' i IDX u 6:8    w linespoint ti columnhead, \
     for [IDX=0:5] ''           i IDX u 6:8:11 w yerrorbars pt 0 ti ''

set output 'time_tes.eps'
set ylabel 'OCR time (s)'
plot for [IDX=0:5] 'output_tes.csv' i IDX u 6:9    w linespoint ti columnhead, \
     for [IDX=0:5] ''           i IDX u 6:9:13 w yerrorbars pt 0 ti ''

